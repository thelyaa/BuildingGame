<head>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link href="/css/style.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/css/font-awesome-4.7.0/css/font-awesome.min.css">
</head>

<div class="financerScreen_allContentBlock">
    <div class="air">
        <div class="form-inner">
            <div class="stripes-block"></div>
            <div id="firmInfo"></div>
            <div class="form-row"></div>
            <div class="buttonMenuBlock">
                <div class="button-menu" onclick="GetRequest()">
                    <div class="icon">
                        <i class="fa fa-search"></i>
                    </div>
                    <div class="icon-text">
                        <span>Рассмотреть заявку</span>
                    </div>
                </div>
            </div>
            <div id="main"></div>
            <div class="form-row"></div>
            <table class="financerScreen_table">
                <th>Материал</th>
                <th>Цены поставщика</th>
                <tr>
                    <td><label>Фундамент</label></td>
                    <td>
                        <div id="contractorPrice1"></div>
                    </td>
                </tr>
                <tr>
                    <td><label>Стены</label></td>
                    <td>
                        <div id="contractorPrice2"></div>
                    </td>
                </tr>
                <tr>
                    <td><label>Крыша</label></td>
                    <td>
                        <div id="contractorPrice3"></div>
                    </td>
                </tr>
            </table>
            <div class="form-row"></div>
            <table class="financerScreen_table">
                <th>Инструмент</th>
                <th>Цены поставщика</th>
                <tr>
                    <td><label>Степлер</label></td>
                    <td>
                        <div id="contractorPrice4"></div>
                    </td>
                </tr>
                <tr>
                    <td><label>Скотч</label></td>
                    <td>
                        <div id="contractorPrice5"></div>
                    </td>
                </tr>
                <tr>
                    <td><label>Скрепки</label></td>
                    <td>
                        <div id="contractorPrice6"></div>
                    </td>
                </tr>
                <tr>
                    <td><label>Скобы для степлера</label></td>
                    <td>
                        <div id="contractorPrice7"></div>
                    </td>
                </tr>
            </table>
            <div class="form-row"></div>
            <div class="buttonMenuBlock">
                <div class="button-menu" onclick="GetPrices()">
                    <div class="icon">
                        <i class="fa fa-info-circle"></i>
                    </div>
                    <div class="icon-text">
                        <span>Узнать цены на материалы</span>
                    </div>
                </div>
            </div>
            <div class="form-row"></div>
        </div>
    </div>
</div>
<script src="../js/financerScript.js"></script>
